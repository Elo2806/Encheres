<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Encheres</display-name>
  <welcome-file-list>
    <welcome-file>accueil</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>JSP_PROFIL</servlet-name>
    <jsp-file>/WEB-INF/jsp/Profil.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_PROFIL</servlet-name>
    <url-pattern>/profil</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSP_ACCUEIL</servlet-name>
    <jsp-file>/WEB-INF/jsp/Accueil.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_ACCUEIL</servlet-name>
    <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSP_ENCHERES</servlet-name>
    <jsp-file>/WEB-INF/jsp/Encheres.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_ENCHERES</servlet-name>
    <url-pattern>/enchere</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSP_COMPTE</servlet-name>
    <jsp-file>/WEB-INF/jsp/Compte.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_COMPTE</servlet-name>
    <url-pattern>/compte</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSP_CONNEXION</servlet-name>
    <jsp-file>/WEB-INF/jsp/Connexion.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_CONNEXION</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSP_VENTE</servlet-name>
    <jsp-file>/WEB-INF/jsp/Ventes.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSP_VENTE</servlet-name>
    <url-pattern>/vente</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletVente</display-name>
    <servlet-name>ServletVente</servlet-name>
    <servlet-class>org.eni.encheres.servlets.ServletProfil</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletVente</servlet-name>
    <url-pattern>/ServletVente</url-pattern>
  </servlet-mapping>
</web-app>